{
    "name": "@iac-factory/api-path-matching",
    "version": "6.2.6",
    "publishConfig": {
        "access": "public"
    },
    "description": "Express style path to RegExp utility",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/pillarjs/path-to-regexp.git"
    },
    "main": "dist/index.js",
