{
    "name": "@cloud-technology/node-react-cdktf-image",
    "private": false,
    "version": "0.1.116",
    "type": "module",
    "types": "declarations",
    "repository": "git+https://github.com/cloud-hybrid/factory.git",
    "license": "BSD 1-Clause License",
    "scripts": {
        "get": "cdktf get",
        "build": "tsc",
        "deploy": "node ci/deploy.js",
        "destroy": "node ci/destroy.js",
        "synth": "cdktf synth",
        "compile": "tsc --pretty --watch",
        "next": "ts-node -r ts-node/register ./src/logging",
        "watch": "tsc -w",
        "test": "jest",
        "upload": "npm publish --access public",
        "reinstall": "npm update && npm install --global --force",
        "prepublishOnly": "npm install",
        "test-watch": "jest --watch",
        "upgrade": "npm i cdktf@latest cdktf-cli@latest",
        "upgrade:next": "npm i cdktf@next cdktf-cli@next"
    },
    "engines": {
        "node": ">=16"
    },
    "engineStrict": true,
    "dependencies": {
        "@cdktf/provider-docker": "^0.6",
        "cdktf": "^0.9",
        "cdktf-cli": "latest",
        "constructs": "^10.0.48",
        "winston": "^3.5.1"
    },
    "devDependencies": {
        "@jest/globals": "latest",
        "@swc/core": "^1.2.138",
        "@swc/helpers": "^0.3.2",
        "@types/jest": "latest",
        "@types/json-schema": "latest",
        "@types/node": "^17.0.17",
        "@types/source-map-support": "^0.5.4",
        "@types/uuid": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/experimental-utils": "latest",
        "@typescript-eslint/parser": "latest",
        "@typescript-eslint/scope-manager": "latest",
        "@typescript-eslint/type-utils": "latest",
        "@typescript-eslint/typescript-estree": "latest",
        "eslint": "latest",
        "jest": "latest",
        "regenerator-runtime": "^0.13.9",
        "ts-jest": "latest",
        "ts-node": "^10.5.0",
        "tslib": "^2.3.1",
        "typescript": "latest",
        "typescript-eslint-language-service": "latest"
    },
    "bin": {
        "node-react-cdktf-image": "ci/synthesize.js"
    },
    "workspaces": {
        "packages": [
            "."
        ],
        "nohoist": [
            "**"
        ]
    }
}
