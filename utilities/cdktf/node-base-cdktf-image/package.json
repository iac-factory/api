{
    "name": "@cloud-technology/node-base-cdktf-image",
    "private": false,
    "version": "0.1.116",
    "type": "module",
    "types": "declarations",
    "repository": "git+https://github.com/cloud-hybrid/factory.git",
    "license": "BSD 1-Clause License",
    "scripts": {
        "get": "cdktf get",
        "build": "tsc",
        "deploy": "node ci/deploy.js",
        "destroy": "node ci/destroy.js",
        "synth": "cdktf synth",
        "compile": "tsc --pretty",
        "watch": "tsc -w",
        "test": "jest",
        "upload": "npm publish --access public",
        "reinstall": "npm update && npm install --global --force",
        "prepublishOnly": "npm install",
        "test-watch": "jest --watch",
        "upgrade": "npm i cdktf@latest cdktf-cli@latest",
        "upgrade:next": "npm i cdktf@next cdktf-cli@next"
    },
    "engines": {
        "node": ">=16"
    },
    "engineStrict": true,
    "dependencies": {
        "@cdktf/provider-docker": "^0.6.3",
        "cdktf": "^0.9",
        "constructs": "^10.0.48"
    },
    "devDependencies": {
        "@jest/globals": "latest",
        "@types/jest": "latest",
        "@types/json-schema": "latest",
        "@types/node": "latest",
        "@types/uuid": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/experimental-utils": "latest",
        "@typescript-eslint/parser": "latest",
        "@typescript-eslint/scope-manager": "latest",
        "@typescript-eslint/type-utils": "latest",
        "@typescript-eslint/typescript-estree": "latest",
        "eslint": "latest",
        "jest": "latest",
        "ts-jest": "latest",
        "ts-node": "latest",
        "tslib": "latest",
        "typescript": "latest",
        "typescript-eslint-language-service": "latest"
    },
    "bin": {
        "node-base-cdktf-image": "ci/synthesize.js"
    }
}
